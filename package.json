{
  "name": "newton-checkbox-tree",
  "version": "1.0.0",
  "description": "A full featured, customizable checkbox tree component. Part of the Newton Common Component family which runs on Zowe plugins.",
  "main": "./dist/commonjs/index.js",
  "module": "./dist/es/index.js",
  "types": "./src/index.d.ts",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "yarn clean:dist && yarn clean:demo && yarn clean:build",
    "clean:dist": "rm -rf ./dist",
    "clean:demo": "rm -rf ./example/dist",
    "clean:build": "rm -rf ./build",
    "start-demo": "./node_modules/.bin/parcel example/src/index.html --out-dir example/dist --open",
    "build-demo": "./node_modules/.bin/parcel build example/src/index.html --out-dir example/dist --public-url ./",
    "publish-demo": "yarn clean:demo && yarn build-demo && gh-pages -d example/dist",
    "build-commonjs": "cross-env BABEL_ENV=commonjs babel src/ --out-dir dist/commonjs --ignore 'src/demo/*','src/stories/*','src/test/*'",
    "build-esmodules": "cross-env BABEL_ENV=esmodules babel src/ --out-dir dist/es --ignore 'src/demo/*','src/stories/*','src/test/*'",
    "build-css": "node-sass src/scss -o dist/css",
    "build-all": "yarn clean:dist && yarn build-commonjs && yarn build-esmodules && yarn build-css",
    "start:dev": "./node_modules/.bin/parcel ./src/demo/index.dev.html --out-dir build --open",
    "start:prod": "yarn build-all && ./node_modules/.bin/parcel ./src/demo/index.prod.html --out-dir build --open",
    "start:examples:basic": "./node_modules/.bin/parcel ./src/demo/pages/basic/index.html --out-dir build --open",
    "start:examples:clickable-labels": "./node_modules/.bin/parcel ./src/demo/pages/clickable-labels/index.html --out-dir build --open",
    "start:examples:custom-icons": "./node_modules/.bin/parcel ./src/demo/pages/custom-icons/index.html --out-dir build --open",
    "start:examples:disabled": "./node_modules/.bin/parcel ./src/demo/pages/disabled/index.html --out-dir build --open",
    "start:examples:expand-all": "./node_modules/.bin/parcel ./src/demo/pages/expand-all/index.html --out-dir build --open",
    "start:examples:hidden-checkboxes": "./node_modules/.bin/parcel ./src/demo/pages/hidden-checkboxes/index.html --out-dir build --open",
    "start:examples:large-data": "./node_modules/.bin/parcel ./src/demo/pages/large-data/index.html --out-dir build --open",
    "start:examples:no-cascade": "./node_modules/.bin/parcel ./src/demo/pages/no-cascade/index.html --out-dir build --open",
    "start:examples:pessimistic-toggle": "./node_modules/.bin/parcel ./src/demo/pages/pessimistic-toggle/index.html --out-dir build --open",
    "test": "cross-env BABEL_ENV=test jest",
    "test:watch": "cross-env BABEL_ENV=test jest --watch",
    "test:details": "cross-env BABEL_ENV=test jest --verbose",
    "test:coverage": "cross-env BABEL_ENV=test jest --coverage --colors",
    "test:snapshot:cleanup": "yarn run test:details -u",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -c .storybook -o .out",
    "build-source-map": "parcel build src/index.js --out-dir build",
    "view-source-map": "source-map-explorer --html build/index.js build/index.js.map > source.html && open source.html"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.0.0-0",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.2.0",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@storybook/addon-actions": "^5.0.1",
    "@storybook/addon-knobs": "^5.0.1",
    "@storybook/addon-links": "^5.0.1",
    "@storybook/addons": "^5.0.1",
    "@storybook/react": "^5.0.1",
    "@types/react": "^16.4.13",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.3.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "babel-register": "^6.26.0",
    "chai": "^4.0.0",
    "connect-history-api-fallback": "^1.6.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-16": "^1.0.0",
    "gh-pages": "^2.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.3.1",
    "jsdom": "^13.1.0",
    "koa-connect": "^2.0.1",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "parcel-bundler": "^1.6.2",
    "sass": "^1.17.2",
    "sass-loader": "^7.1.0",
    "source-map-explorer": "^1.7.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.4.1",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1",
    "webpack-node-externals": "^1.6.0",
    "webpack-stream": "^5.0.0"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "lodash": "^4.17.10",
    "nanoid": "^2.0.0",
    "prop-types": "^15.5.8",
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  }
}
